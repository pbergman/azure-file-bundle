            "name": "Philip Bergman",
            "email": "pbergman@live.nl"
        }
    ],
    "require": {
        "php": "^7.4||^8.0",
        "symfony/cache-contracts": "^1.1||^2.4||^3.0",
        "symfony/http-client-contracts": "^1.1||^2.4||^3.0",
        "symfony/config": "^4.4||^5.0||^6.0",
        "symfony/console": "^4.4||^5.0||^6.0",
